{
  "extends": [
    "prettier",
    "next/core-web-vitals",
    "plugin:storybook/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "quotes": [
      "error",
      "backtick"
    ],
    "prettier/prettier": "error",
    "semi": "off",
    "@next/next/no-page-custom-font": "off",
    "react/no-unescaped-entities": "off",
    "react-hooks/exhaustive-deps": "off"
  }
}
